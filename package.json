{
    "name": "MoonPunk",
    "version": "1.0.0",
    "description": "Character Sheet for Moonpunk",
    "main": "index.js",
    "dependencies": {
      "pug": "^2.0.4",
      "node-sass": "^4.5.3",
      "npm-run-all": "^4.1.1"
    },
    "scripts": {
      "start": "npm-run-all --parallel compile:sass compile:pug",
      "compile:sass": "node-sass moonpunk.scss moonpunk.css -w",
      "compile:pug": "pug moonpunk.pug moonpunk.html -w"
    },
    "repository": {
      "type": "git",
      "url": "git+https://github.com/stevenpaulr/MoonPunk.git"
    },
    "keywords": [
      "Roll20"
    ],
